5.def right_angled(a, b, c):
    if (a*a+b*b==c*c) or (c*c+b*b==a*a) or (a*a+c*c==b*b) :
        return "The triangle is right-angled." 
    else:
        return "The triangle is not right-angled."
right_angled(3,4,5)
6.string=input("Enter a string")
7.list=[i for i in string]
print(list)
def listToString(s):
    str1 = ""
    for l in s:
        str1 += l
    return str1
s = ['I', 'am', 'student']
print(listToString(s))
