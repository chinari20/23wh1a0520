1.def vol_cube(a):
    volume = a**3
    return volume
s=int(input("Enter side"))
vol_cube(s)
2.def rainbow(s):
    colours = ""
    return(colours.join(s))
s = [" Voilet"," Indigo"," Blue"," Green"," Yellow"," Orange"," Red"]
print(rainbow(s))
